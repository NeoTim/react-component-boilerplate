{
  "name": "common-lib-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "webpack:dll": "rimraf dll/* && webpack --config config/webpack.config.dll.js",
    "example": "npm run webpack:dll && node config/webpack.dev.server.js",
    "clean:lib": "rimraf lib/*",
    "build": "npm run clean:lib && rollup -c",
    "build:watch": "npm run clean:lib && rollup -c -w",
    "compile": "npm run clean:lib && tsc",
    "compile:watch": "npm run clean:lib && tsc -w",
    "format": "prettier --write \"{examples_src,src}/**/*.{js,ts,tsx}\"",
    "format:check": "prettier --list-different \"{example_src,src}/**/*.{js,ts,tsx}\"",
    "lint": "tslint -c tslint.json \"src/**/*.{ts,tsx}\"",
    "lint:fix": "tslint -c tslint.json \"src/**/*.{ts,tsx}\" --fix",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "release": "standard-version"
  },
  "lint-staged": {
    "{examples_src,src}/**/*.{js,ts,tsx}": [
      "npm run format",
      "npm run lint:fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/forsigner/common-lib-boilerplate.git"
  },
  "author": "forsigner",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/forsigner/common-lib-boilerplate/issues"
  },
  "homepage": "https://gitlab.com/forsigner/common-lib-boilerplate#README",
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "glob": "^7.1.2",
    "html-webpack-harddisk-plugin": "^0.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "ip": "^1.1.5",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-typescript": "^0.8.1",
    "standard-version": "^4.4.0",
    "style-loader": "^0.22.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1",
    "webpack": "^4.16.5",
    "webpack-command": "^0.4.1",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3"
  },
  "peerDependencies": {
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
